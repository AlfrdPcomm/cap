    ]
  },*/
  "menu-item#item5":{
    "props":{
      "text":"Itemn 5"
    },
    "blocks":[
      "vtex.menu@2.x:submenu#categ"
    ]
  },
  "vtex.menu@2.x:submenu#categ":{
    "children":[
      "vtex.menu@2.x:submenu#cat1"
    ]
  },
  "vtex.menu@2.x:submenu#cat1":{
    "props":{
      "type":"category",
      "categoryId":3,
      "orientation": "vertical"
    }
  },
  
  "vtex.menu@2.x:submenu#submenuPrueba":{
    "children":[
