  "flex-layout.row#catmenu":{
    "children":[
      "category-menu"
    ]
  },
  "category-menu":{
    "props": {
      "showAllDepartments": false,
      "showSubcategories": false,
      "menuDisposition": "center",
      "departments": [
        1, 2, 3, 48, 35
      ],
      "sortSubcategories": "name"
    }
  },
  
  "flex-layout.row#menuPrueba":{
    "children":[
      "flex-layout.col#col1",
      "flex-layout.col#col2",
      "flex-layout.col#col3"
    ]
  },
  "flex-layout.col#col1":{
    "children":[
      "vtex.menu@2.x:menu#row1col1"
    ]
  },
  "vtex.menu@2.x:menu#row1col1":{
    "children":[
      "drawer#home",
      "menu-item#item1row1col1",
      "menu-item#item2row1col1",
      "menu-item#item3row1col1",
      "menu-item#item4row1col1",
      "menu-item#item5"
    ],
    "props":{
      //"orientation": "vertical"
    }
  },
  "drawer#home":{
    "children":[
      "menu#drawerHome"
    ]
  },
  "menu#drawerHome":{
    "children":[
      "menu-item#item1row1col1",
      "menu-item#item2row1col1",
      "menu-item#item3row1col1",
      "menu-item#item4row1col1"
    ],
    "props":{
      "orientation": "vertical"
    }
  },

  "menu-item#item1row1col1":{
    "props":{
      "type":"custom",
      "itemProps":{
        "text":"Item 1 - row 1 - col 1",
        "href":"/sika",
        "tagTitle": "Ejemplo de label"
      }
    }
  },
  "menu-item#item2row1col1":{
    "props":{
      "type":"category",
      "categoryId":3,
      "itemProps":{
        "text":"Item 2 - row 1 - col 1"
      }
    }
  },
  "menu-item#item3row1col1":{
    "props":{
      "itemProps":{
        "text":"Item 3 - row 1 - col 1"
      }
    },
    "blocks":[
    ]
